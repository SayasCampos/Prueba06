<section id="sample" style="background: url(img/background/car-radio.jpeg) center center no-repeat; background-size: cover;" class="sample-test">





  <div class="container">


    <div class="row">
        <div class="col-md-2">
            <button  id="radio-player" class="btn btn-secondary btn-lg"type="submit" onclick="requestFullScreen()">
            <div class="icon"><span style="font-size: 3em; color: LightBlue;"><img src="img/album/opb.jpg" width="50" height="50"></span></div>
            <h3 data-animate="fadeInUp" class="heading mb-3 text-400">News</h3>
            </button>
        </div>
        <div class="col-md-2">
            <button  id="radio-player" class="btn btn-secondary btn-lg"type="submit" onclick="requestFullScreen()">
            <div class="icon"><span style="font-size: 3em; color: LightBlue;"><img src="img/album/groovesalad.jpg" width="50" height="50"></span></div>
            <h3 data-animate="fadeInUp" class="heading mb-3 text-400">Electro</h3>
            </button>            
        </div>
        <div class="col-md-2">
            <button  id="radio-player" class="btn btn-secondary btn-lg"type="submit" onclick="requestFullScreen()">
